{#
/**
 * @file
 * Theme override for a single local action link.
 *
 * Available variables:
 * - attributes: HTML attributes for the wrapper element.
 * - link: A rendered link element.
 *
 * @see template_preprocess_menu_local_action()
 * @see uikit_preprocess_menu_local_action()
 *
 * @ingroup uikit_themeable
 */
#}
{% set title %}
  <i uk-icon="icon: plus; ratio: .75;"></i> {{ link.title }}
{% endset %}
<li{{ attributes }}>
  <a href="{{ link.url }}" class="uk-button uk-button-primary uk-button-small uk-text-capitalize">{{ title }}</a>
</li>
